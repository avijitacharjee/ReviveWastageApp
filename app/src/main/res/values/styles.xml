<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentTransitions">true</item>
    </style>
    <style name="Widget.Custom.NavigationView" parent="Widget.Design.NavigationView">
        <item name="itemIconTint">@color/black_1</item>
        <item name="itemTextColor">@color/black_1</item>
        <item name="itemBackground">@color/white_1</item>
        <item name="android:textColorSecondary">@color/black</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextAppearance44">
        <item name="android:textColor">@color/black</item>
    </style>

</resources>